{
    "manifest_version": 2,
   
    "name": "Links s3 addresses as URLs",
    "version": "1.0",
    "description": "Changes the text on the page that describes the S3 address into a clickable link",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["http://*/*","https://*/*"]
    }],

    "icons": { 
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
   
  }